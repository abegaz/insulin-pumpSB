<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MainController">
   <children>
      <LineChart fx:id="LineChart" prefHeight="400.0" prefWidth="590.0" title="Blood Sugar Graph">
        <xAxis>
          <CategoryAxis label="Time" prefHeight="24.0" prefWidth="444.0" side="BOTTOM" fx:id="x" />
        </xAxis>
        <yAxis>
          <NumberAxis fx:id="y" autoRanging="false" label="Blood Sugar Level" lowerBound="40.0" minorTickCount="10" side="LEFT" tickLabelGap="5.0" upperBound="160.0">
               <tickLabelFont>
                  <Font size="10.0" />
               </tickLabelFont></NumberAxis>
        </yAxis>
      </LineChart>
   </children>
</AnchorPane>
